{
  "name": "Tab Punisher",
  "description": "Bothers you when you are about to get distracted.",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
          "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
     "16": "icon.png",
     "32": "icon.png",
     "64": "icon.png",
     "128": "icon.png"
  },
  "version": "0.0.1",
  "minimum_chrome_version": "44",
  "manifest_version": 2
}
